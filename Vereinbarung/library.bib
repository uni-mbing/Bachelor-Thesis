	pages = {907190},
	file = {Gillette et al_2022_A personalized real-time virtual model of whole heart electrophysiology.pdf:/home/valentin/Zotero/storage/VNKH8WC8/Gillette et al_2022_A personalized real-time virtual model of whole heart electrophysiology.pdf:application/pdf},
}

