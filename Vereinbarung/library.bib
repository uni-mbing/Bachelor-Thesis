	pages = {12768},
	file = {Full Text:/home/valentin/Zotero/storage/YJQV6CSN/Kubitschke et al. - 2019 - Roadmap to Local Tumour Growth Insights from Cerv.pdf:application/pdf},
}

@article{deisboeck_multiscale_2011,
